<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.banking_system.AccountController">
    <children>
        <TableView fx:id="accountTable" layoutX="50.0" layoutY="50.0" prefHeight="300.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="accNumColumn" prefWidth="120.0" text="ACCOUNT NUMBER" />
                <TableColumn fx:id="balanceColumn" prefWidth="120.0" text="BALANCE" />
                <TableColumn fx:id="custIdColumn" prefWidth="120.0" text="CUSTOMER ID" />
                <TableColumn fx:id="accTypeColumn" prefWidth="120.0" text="ACCOUNT TYPE" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
